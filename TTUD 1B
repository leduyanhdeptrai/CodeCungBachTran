#include <bits/stdc++.h>

using namespace std;

int n, a[1000005];
long long ans, sum;
int main()
{
    cin >> n;
    for(int i = 1; i <= n; ++i) cin >> a[i];
    for(int i = 1; i <=n ; ++i){
      sum += a[i];
      ans = max (ans,sum);
      if(sum < 0) sum = 0;
    }
    cout << ans;
    return 0;
}
